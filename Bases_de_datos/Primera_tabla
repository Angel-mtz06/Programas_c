CREATE DATABASE angel;
USE angel;
USE DATABASE angel;
CREATE TABLE IF NOT EXISTS empleados (
	numero INT NOT NULL,
    nombre VARCHAR (50),
    salario DECIMAL (6, 2) ZEROFILL,
    categoria CHAR (20),
    sexo ENUM ('M', 'F'),
    departamento CHAR (2),
    PRIMARY KEY (numero) TYPE = MYISAM
);
SHOW TABLES;
DESCRIBE empleados;
CREATE TABLE IF NOT EXISTS departamentos (
    codigo CHAR (2) NOT NULL,
    nombre VARCHAR (50) NOT NULL,
    PRIMARY KEY (codigo) ENGINE = INODB); 